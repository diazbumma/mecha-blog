<%- include('partials/header') %>

<h1>You're at the home page</h1>

<% blogs.forEach(item => { %>
    <div>
        <h2><%= item.title %></h2>
        <img src="<%= item.image %>">
        <span><%= item.created.toDateString() %></span>
        <p><%= item.body.substring(0, 100) + '..' %></p>
        <a href="/blogs/<%= item._id %>">Read more</a>
        <a href="/blogs/<%= item._id %>/edit">Edit post</a>
    </div>
<% }); %>

<%- include('partials/footer') %>